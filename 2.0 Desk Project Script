  /TABLES= cohort_AD_control BY AHT_nondiuretic
  /FORMAT=AVALUE TABLES
    /STATISTICS=CHISQ 
  /CELLS=COUNT ROW PROP 
  /COUNT ROUND CELL
  /BARCHART.

#Within HT cases, significant association between non-diuretic AHM and AD (X2(1) = 46.600, 
p = 0.000).
#Of the AD cases with HT, 25.00 % receive AHT.
#Of the Control cases with HT, 76.47 % receive AHT.

CROSSTABS
  /TABLES=cohort_AD_control BY ACEI_ARB 
  /FORMAT=AVALUE TABLES
    /STATISTICS=CHISQ 
  /CELLS=COUNT ROW PROP 
  /COUNT ROUND CELL
  /BARCHART.

#Within HT cases, significant association between ACEI/ARB treatment and AD (X2(1) = 24.265, 
p = 0.000).
#Of the AD cases with HT, 10.53 % receive AHT.
#Of the Control cases with HT, 45.88 % receive AHT.

