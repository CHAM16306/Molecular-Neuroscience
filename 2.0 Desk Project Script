  /METHOD=ENTER cohort_AD_control 
  /CONTRAST (cohort_AD_control)=Indicator(1)
  /CLASSPLOT
  /PRINT=CORR CI(95)
  /CRITERIA=PIN(0.05) POUT(0.10) ITERATE(20) CUT(0.5).



LOGISTIC REGRESSION VARIABLES AHT_nondiuretic
  /METHOD=ENTER cohort_AD_control 
  /CONTRAST (cohort_AD_control)=Indicator(1)
  /CLASSPLOT
  /PRINT=CORR CI(95)
  /CRITERIA=PIN(0.05) POUT(0.10) ITERATE(20) CUT(0.5).



LOGISTIC REGRESSION VARIABLES ACEI_ARB
  /METHOD=ENTER cohort_AD_control 
  /CONTRAST (cohort_AD_control)=Indicator(1)
  /CLASSPLOT
  /PRINT=CORR CI(95)
  /CRITERIA=PIN(0.05) POUT(0.10) ITERATE(20) CUT(0.5).



